.homePage-list {
  background: #eee;
  height: 100%;
  width: 100%;
}
button.shouqaun{
  position:relative;
display:flex;
align-items: center;
justify-content: center;
margin-left:auto;
margin-right:auto;
padding-left:14px;
padding-right:14px;
box-sizing:border-box;
font-size:18px;
line-height:2.55555556;
border-radius:0;
-webkit-tap-highlight-color:transparent;
overflow:hidden;
color:#fff;
background-color:rgba(0, 0, 0,0.4);

}
.list-home {
  height: 100%;
  display: flex;
  flex-direction: column;
  overflow-y: scroll;
}

.erweima {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  position: absolute;
  top: 0px;
  z-index: 999;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  color: white;
}

.erweima image {
  max-width: 400rpx;
  max-height: 400rpx;
}

.fontSize {
  font-size: 30rpx;
  /* flex: 6; */
}

.banner {
  height: 350rpx;
  width: 100%;
  position: relative;
  /* display: flex; */
}

.banner-top {
  position: absolute;
  top: 0px;
  width: 100%;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  background: rgba(0, 0, 0, 0.4);
  color: white;
  font-size: 28rpx;
}

.banner-top text {
  margin: 0 auto;
  width: 90%;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-align: center;
}

.banner-middle {
  position: absolute;
  left: 50%;
  width: 194rpx;
  height: 66rpx;
  margin-left: -97rpx;
  line-height: 66rpx;
  text-align: center;
  color: white;
  background: #4a6772;
  top: 214rpx;
}

.home-content {
  width: 100%;
  /* float: left; */
  flex: 1;
}

.home-content-top {
  background: #fff;
  margin-bottom: 18rpx;
}

.content-line {
  padding: 30rpx 20rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #e1e1e1;
}

.songhuo {
  width: 30rpx;
  height: 30rpx;
  margin-right: 5px;
}

.youjiantou {
  width: 20rpx;
  height: 30rpx;
  margin-left: 14rpx;
}

.content-line-right {
  display: flex;
  align-items: center;
  /* flex: 3; */
  text-align: right;
}

.content-card {
  display: flex;
  align-items: center;
  padding: 30rpx 0rpx;
}

.content-card-w20 {
  flex: 1;
  position: relative;
  text-align: center;
}

.content-card-w20 image {
  width: 38rpx;
  height: 40rpx;
}

.content-card-w20 text {
  font-size: 24rpx;
}

.message-weidu {
  font-size: 28rpx;
  color: #666;
}

/*首页消息列表  */

.message-list {
  width: 100%;
}

.message-li {
  display: flex;
  align-items: center;
  padding: 32rpx 20rpx 32rpx 20rpx;
  border-bottom: 1px solid #e1e1e1;
}

.message-imgLeft {
  position: relative;
}

.message-imgLeft image {
  width: 100rpx;
  height: 100rpx;
}

.message-right {
  flex: 1;
  padding-left: 20rpx;
}

.message-title {
  font-size: 28rpx;
  color: #000;
  margin-bottom: 16rpx;
}

.message-content {
  width: 90%;
  color: #666;
  font-size: 26rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.circle {
  border: 1rpx solid #dd2727;
  background: #dd2727;
  color: white;
  font-size: 24rpx;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 40rpx;
  position: absolute;
  top: -10px;
  left: 50%;
}

.more {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  display: flex;
  justify-content: center;
  font-size: 32rpx;
  color: #666;
}

